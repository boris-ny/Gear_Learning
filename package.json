{
  "name": "gearlearning",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev-api\" \"npm run dev-client\"",
    "dev-api": "nodemon api/dev.js",
    "dev-client": "cd client && yarn dev",
    "start": "node index.js",
    "test": "cd api && jest --collectCoverage=true --forceExit",
    "test:watch": "cd api && jest --watch --collectCoverage=true --runInBand --detectOpenHandles"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
